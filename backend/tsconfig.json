{
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "jsx": "react",
      "declaration": true,
      "rootDir": "./src",
      "types": ["sequelize/types"]
    },
    "include": [
      "./src/database",
      "src/**/*.ts",
      "src/**/*.js"
      
, "src/routes"    ],
    "exclude": [
      "node_modules",
      "dist"
    ]
  }
  